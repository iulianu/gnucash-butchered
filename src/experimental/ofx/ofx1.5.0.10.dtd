<!--#ELEMENT INTRACANRQ #Link(SRVRTID2,BANKMSGSETV2)-->


<!-- interbank transfer request -->
<!ELEMENT INTERRQ - - (XFERINFO)> 


<!-- interbank transfer modification request -->
<!-- version 2 uses SRVRTID2, version 1 SRVRTID -->
